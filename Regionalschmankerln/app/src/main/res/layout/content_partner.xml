<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/app_bar_partner"
    tools:context="de.armin.android.regionalschmankerln.Partner">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="HTL Innsbruck, AnichstraÃŸe"
        android:id="@+id/textView3"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="20dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:id="@+id/id_htllogo"
        android:layout_below="@+id/textView3"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        android:onClick="clickhtllogo"
        android:src="@drawable/htllogo" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Wirtschaftskammer Kufstein"
        android:id="@+id/textView4"
        android:layout_below="@+id/id_htllogo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="35dp"
        android:textSize="20dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="120dp"
        android:id="@+id/id_wklogo"
        android:layout_below="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:src="@drawable/wklogo"
        android:clickable="true"
        android:onClick="clickwklogo"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Landwirtschaftskammer Kufstein"
        android:id="@+id/textView5"
        android:layout_below="@+id/id_wklogo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="35dp"
        android:textSize="20dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:id="@+id/id_lklogo"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true"
        android:src="@drawable/lktirollogo"
        android:clickable="true"
        android:onClick="clicklklogo"/>
</RelativeLayout>
